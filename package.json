{
  "name": "@braebo/docinator",
  "version": "0.1.0",
  "description": "Generates documentation from TypeScript / Svelte files.",
  "keywords": [],
  "license": "MIT",
  "author": "braebo",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.ts.d.ts",
      "default": "./dist/index.ts.js"
    }
  },
  "types": "./dist/index.ts.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "vite-node --watch src/index.ts",
    "build": "tsup",
    "build:watch": "tsup --watch",
    "lint": "publint"
  },
  "devDependencies": {
    "@changesets/cli": "2.27.7",
    "@shikijs/transformers": "^1.12.1",
    "@svitejs/changesets-changelog-github-compact": "1.1.0",
    "@types/marked": "^6.0.0",
    "@types/node": "22.1.0",
    "chalk": "^5.3.0",
    "changeset": "0.2.6",
    "marked": "^14.0.0",
    "marked-highlight": "^2.1.4",
    "publint": "0.2.9",
    "shiki": "^1.12.1",
    "tsup": "8.2.4",
    "vite-node": "2.0.5",
    "vitest": "2.0.5"
  },
  "peerDependencies": {
    "typescript": "latest"
  }
}